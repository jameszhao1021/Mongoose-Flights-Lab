<%- include('../partials/header') %>
<table>
   <thead>
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>Flight No</th>
        <th>Departs</th>
      </tr>
</thead>
   <tbody>
   <% flights.forEach((f)=>{ %>
    <tr>
        <td><%= f.airline %></td>
        <td><%= f.airport %></td>
        <td><%= f.flightNo %></td>
        <td><%= f.departs.toLocaleString() %></td>
      </tr>
    <%  })%>
   </tbody>
</table>


<%- include('../partials/footer') %>