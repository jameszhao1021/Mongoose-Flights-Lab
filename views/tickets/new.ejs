<%- include('../partials/header') %>

<form action="/flights/<%= flight._id %>/tickets/new" method="POST">
      <label>Seat: </label>
      <input name="seat" required pattern="[A-F][1-9]\d?" value="A1 - F99">
      <label>Price: </label>
      <input type="number" name="price">
      <input type="submit" value="Add Ticket">

    </form>
